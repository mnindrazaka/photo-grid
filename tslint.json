{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never"
    },
    "no-console": false,
    "interface-name": {
      "options": "always-prefix"
    },
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false
  },
  "rulesDirectory": []
}
